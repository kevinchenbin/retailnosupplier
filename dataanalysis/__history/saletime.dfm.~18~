  object aqprint: TADOQuery
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'starttime'
        DataType = ftString
        Value = Null
      end
      item
        Name = 'endtime'
        DataType = ftString
        Value = Null
      end>
    SQL.Strings = (
      
        'select '#39#38646#21806#39' R0, sum(BS_RetailNoteHeader.TotalDiscountedprice) as' +
        ' R1 , '
      
        '     sum(BS_RetailNoteHeader.TotalFixedprice) as R2,sum(BS_Retai' +
        'lNoteHeader.Totalamount) as R3,sum((CASE WHEN dbo.BS_RetailNoteH' +
        'eader.TotalDiscountedPrice > (RecCash + RecCardMoney) '
      
        'THEN (RecCash + RecCardMoney) ELSE dbo.BS_RetailNoteHeader.Total' +
        'DiscountedPrice END)) as R4,0.0 R5'
      ' from BS_RetailNoteHeader'#9'where '
      #9' (:starttime is null or datediff(day,:starttime,HdTime) >= 0)'
      #9'and (:endtime is null or datediff(day,:endtime,HdTime) <= 0)'
      'union all '
      '     '
      
        #9'select '#39#25209#38144#39' R0 ,sum(BS_WsaleNoteHeader.Checked) as R1 , sum(BS_' +
        'WsaleNoteHeader.Nochecked) as R2,sum(BS_WsaleNoteHeader.totalamo' +
        'unt) as R3,sum(BS_WsaleNoteHeader.discountedprice) as R4,'
      'sum(BS_WsaleNoteHeader.fixedprice) as R5'
      #9'FROM  BS_WsaleNoteHeader'
      
        #9'where (:starttime is null or datediff(day,:starttime,HdTime) >=' +
        ' 0)'
      #9'and (:endtime is null or datediff(day,:endtime,HdTime) <= 0)')
    Left = 552
    Top = 232
  end
